APP_NAME="Resend Laravel Examples"
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost:8000

# Resend Configuration
RESEND_API_KEY=re_xxxxxxxxx

# Mail Configuration (using Resend)
MAIL_MAILER=resend
MAIL_FROM_ADDRESS="onboarding@yourdomain.com"
MAIL_FROM_NAME="Acme"

# Webhook Secret
RESEND_WEBHOOK_SECRET=whsec_xxxxxxxxx

# Contact email for notifications
CONTACT_EMAIL=team@yourdomain.com

# Audience ID (for contacts management)
RESEND_AUDIENCE_ID=aud_xxxxxxxxx

# Template ID (for template examples)
RESEND_TEMPLATE_ID=tpl_xxxxxxxxx
